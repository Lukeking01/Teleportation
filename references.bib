	journal = {Nature Reviews Physics},
	number = {6},
	pages = {339--353},
	title = {Progress in quantum teleportation},
	url = {https://doi.org/10.1038/s42254-023-00588-x},
	volume = {5},
	year = {2023},
	bdsk-url-1 = {https://doi.org/10.1038/s42254-023-00588-x}}

