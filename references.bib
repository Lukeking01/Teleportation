	journal3 = {Optica},
	keywords = {Quantum detectors; Quantum communications; Quantum information and processing; Mode-locked fiber lasers; Optical fibers; Photonic entanglement; Quantum key distribution; Quantum teleportation; Superconducting nanowire single photon detectors},
	number = {10},
	pages = {832--835},
	publisher = {Optica Publishing Group},
	title = {Quantum teleportation over 100 km of fiber using highly efficient superconducting nanowire single-photon detectors},
	url = {https://opg.optica.org/optica/abstract.cfm?URI=optica-2-10-832},
